('1.0.2.3.0', '\\begin{aligned}\n    &s_{T \\rightarrow 1} = \\left\\langle {\\bm{W}}_Q {\\bm{v}}_T, {\\bm{W}}_K {\\bm{v}}_1 \\right\\rangle\n    \\\\\n    =& {{\\bm{e}}^{(0)}}^\\top {\\bm{W}}_{Qe} {\\bm{W}}_{Ke} {\\bm{e}}^{(1)}+ \\frac{{{\\bm{e}}^{(0)}}^\\top {\\bm{W}}_{Qe}^\\top {\\bm{W}}_{Kp}}{C} + \\frac{({\\bm{e}}^{(1)})^\\top {\\bm{W}}_{Ke} {\\bm{W}}_{Qp}}{C} \\cdot T + \\frac{{\\bm{W}}_{Qp}^\\top {\\bm{W}}_{Kp}}{C^2} \\cdot T\n    \\\\\n    =& \\left(\\frac{({\\bm{e}}^{(1)})^\\top {\\bm{W}}_{Ke} {\\bm{W}}_{Qp}}{C} + \\frac{{\\bm{W}}_{Qp}^\\top {\\bm{W}}_{Kp}}{C^2}\\right) \\cdot T + O(1)\n    = O(T),\n\\end{aligned}')
('1.0.2.3.0', '\\begin{aligned}\n    &s_{T \\rightarrow T} = \\left\\langle {\\bm{W}}_Q {\\bm{v}}_T, {\\bm{W}}_K {\\bm{v}}_T \\right\\rangle\n    \\\\\n    =& {{\\bm{e}}^{(0)}}^\\top {\\bm{W}}_{Qe} {\\bm{W}}_{Ke} {\\bm{e}}^{(0)}+ \\frac{{{\\bm{e}}^{(0)}}^\\top {\\bm{W}}_{Qe}^\\top {\\bm{W}}_{Kp}}{C} T + \\frac{{{\\bm{e}}^{(0)}}^\\top {\\bm{W}}_{Ke} {\\bm{W}}_{Qp}}{C} \\cdot T + \\frac{{\\bm{W}}_{Qp}^\\top {\\bm{W}}_{Kp}}{C^2} \\cdot T^2\n    \\\\\n    =& \\frac{{\\bm{W}}_{Qp}^\\top {\\bm{W}}_{Kp}}{C^2} \\cdot T^2 + O(T).\n\\end{aligned}')

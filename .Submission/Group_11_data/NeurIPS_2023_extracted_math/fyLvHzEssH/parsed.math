('9', "\\begin{aligned}\n\\begin{split}\n\\label{eq:GP_dot_Q_1_relu}\n    \\dot{Q}_{GP-ReLU}^{(1)}(\\mathbf{x}^{c,i}, \\mathbf{x}^{c',j}) &= \\frac{1}{2\\pi}\\left(\\pi -  \\theta \\right)\n\\end{split}\n\\end{aligned}")
('9', "\\Theta^{(2)}_{NTK-ReLU}(\\mathbf{x}^{c,i}, \\mathbf{x}^{c',j}) = K_{GP-ReLU}^{(2)}(\\mathbf{x}^{c,i}, \\mathbf{x}^{c',j}) + K_{GP}^{(1)}(\\mathbf{x}^{c,i}, \\mathbf{x}^{c',j})\\dot{Q}^{(1)}_{GP-ReLU}(\\mathbf{x}^{c,i}, \\mathbf{x}^{c',j}).")

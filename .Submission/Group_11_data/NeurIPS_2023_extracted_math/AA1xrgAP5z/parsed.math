('11.1', '\\begin{aligned}\n      \\textsc{Opt-Gap} \\le {} & \\frac{1}{\\eta_1} \\mathcal{D}_{\\psi}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_1) + \\sum_{t=1}^T\\left( \\frac{1}{\\eta_{t+1}} - \\frac{1}{\\eta_t} \\right) \\mathcal{D}_{\\psi}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_{t+1})\\\\\n      \\le {} & \\frac{1}{4} (\\gamma + \\lambda) D^2 + \\frac{\\lambda}{4} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|^2.\n  \n\\end{aligned}')
('11.1', '\\begin{aligned}\n      \\textsc{Opt-Gap} - \\textsc{Negativity} \n      \\le {} & \\frac{1}{4} (\\gamma + \\lambda) D^2 + \\frac{\\lambda}{4} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|^2 - \\frac{\\lambda}{2} \\sum_{t=1}^T\\undefined {\\mathbf{x}_t - \\mathbf{x}^\\star}^2\\\\\n      \\le {} & \\frac{1}{4} \\gamma D^2 + \\frac{\\lambda}{2} \\sum_{t=1}^T\\|\\mathbf{x}_t - \\widehat{\\mathbf{x}}_{t+1}\\|^2 + \\mathcal{O}(1).\n  \n\\end{aligned}')
('11.1', '\\frac{\\lambda}{2} \\sum_{t=1}^T\\|\\mathbf{x}_t - \\widehat{\\mathbf{x}}_{t+1}\\|^2 \\le \\frac{\\lambda}{2} \\sum_{t=1}^T\\eta_t^2 \\|\\nabla f_t(\\mathbf{x}_t) - \\mathbf{m}_t\\|^2 \\le \\sum_{t=1}^T\\eta_t \\|\\nabla f_t(\\mathbf{x}_t) - \\mathbf{m}_t\\|^2.')

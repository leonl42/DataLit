('11.1', '\\begin{aligned}\n    \\textsc{Adaptivity} = {} & \\sum_{t=1}^T\\eta_t \\|\\nabla f_t(\\mathbf{x}_t) - \\mathbf{m}_t\\|^2 \\le D \\sum_{t=1}^T\\frac{\\|\\nabla f_t(\\mathbf{x}_t) - \\nabla f_{t-1}(\\mathbf{x}_{t-1})\\|^2}{\\sqrt{1 + \\sum_{s=1}^{t-1} \\|\\nabla f_s(\\mathbf{x}_s) - \\nabla f_{s-1}(\\mathbf{x}_{s-1})\\|^2}}\\\\\n    \\le {} & 4 D \\sqrt{1 + \\sum_{t=1}^T\\|\\nabla f_t(\\mathbf{x}_t) - \\nabla f_{t-1}(\\mathbf{x}_{t-1})\\|^2} + 4DG^2,\n  \n\\end{aligned}')
('11.1', '\\sum_{t=1}^Ta_t/\\sqrt{1 + \\sum_{s=1}^{t-1} a_s} \\le 4 \\sqrt{1 + \\sum_{t=1}^Ta_t} + \\max_{t \\in [T]} a_t')
('11.1', '\\begin{aligned}\n      \\textsc{Opt-Gap} = {} & \\sum_{t=1}^T\\frac{1}{\\eta_t} (\\mathcal{D}_{\\psi}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_t) - \\mathcal{D}_{\\psi}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_{t+1})) = \\sum_{t=1}^T\\frac{1}{2 \\eta_t} (\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_t\\|^2 - \\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|^2)\\\\\n      \\le {} & \\frac{\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_1\\|^2}{2 \\eta_1} + \\sum_{t=2}^T\\left( \\frac{1}{2\\eta_t} - \\frac{1}{2\\eta_{t-1}} \\right) \\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_t\\|^2\\\\\n      \\le {} & \\frac{D}{2} (1 + \\gamma D) + D^2 \\sum_{t=2}^T\\left( \\frac{1}{2\\eta_t} - \\frac{1}{2\\eta_{t-1}} \\right) \\le \\frac{D}{2} (1 + \\gamma D) + \\frac{D^2}{2 \\eta_T}\\\\\n      = {} & \\gamma D^2 + \\frac{D}{2} \\sqrt{1 + \\bar{V}_T} + \\mathcal{O}(1).\n  \n\\end{aligned}')
('11.1', '\\begin{aligned}\n    \\textsc{Stability} = {} & \\sum_{t=1}^T\\frac{1}{2\\eta_t} (\\|\\widehat{\\mathbf{x}}_{t+1} - \\mathbf{x}_t\\|^2 + \\|\\mathbf{x}_t - \\widehat{\\mathbf{x}}_t\\|^2) \\ge \\sum_{t=2}^T\\frac{1}{2\\eta_t} (\\|\\widehat{\\mathbf{x}}_t - \\mathbf{x}_{t-1}\\|^2 + \\|\\mathbf{x}_t - \\widehat{\\mathbf{x}}_t\\|^2)\\\\\n    \\ge {} & \\sum_{t=2}^T\\frac{1}{4\\eta_t} \\|\\mathbf{x}_t - \\mathbf{x}_{t-1}\\|^2 \\ge \\frac{\\gamma}{4} \\sum_{t=2}^T\\|\\mathbf{x}_t - \\mathbf{x}_{t-1}\\|^2.\n  \n\\end{aligned}')
('11.1', '\\mathbf{x}^\\star\\in \\mathop{\\mathrm{arg\\,min}}_{\\mathbf{x}\\in \\mathcal{X}} \\sum_{t=1}^Tf_t(\\mathbf{x})')
('11.1', '\\begin{aligned}\n    \\sum_{t=1}^Tf_t(\\mathbf{x}_t) - \\sum_{t=1}^Tf_t(\\mathbf{x}^\\star) \\le \\underbrace{\\sum_{t=1}^T\\|\\nabla f_t(\\mathbf{x}_t) - \\mathbf{m}_t\\|^2_{U_t^{-1}}}_{\\textsc{Adaptivity}} + \\underbrace{\\sum_{t=1}^T(\\mathcal{D}_{\\psi_t}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_t) - \\mathcal{D}_{\\psi_t}(\\mathbf{x}^\\star, \\widehat{\\mathbf{x}}_{t+1}))}_{\\textsc{Opt-Gap}} &\\\\\n    - \\underbrace{\\sum_{t=1}^T(\\mathcal{D}_{\\psi_t}(\\widehat{\\mathbf{x}}_{t+1}, \\mathbf{x}_t) + \\mathcal{D}_{\\psi_t}(\\mathbf{x}_t, \\widehat{\\mathbf{x}}_t))}_{\\textsc{Stability}} - \\underbrace{\\frac{\\alpha}{2} \\sum_{t=1}^T\\undefined {\\mathbf{x}_t - \\mathbf{x}^\\star}^2_{\\nabla f_t(\\mathbf{x}_t) \\nabla f_t(\\mathbf{x}_t)^\\top}}_{\\textsc{Negativity}}&,\n  \n\\end{aligned}')
('11.1', '\\begin{aligned}\n      \\textsc{Opt-Gap} = {} & \\frac{1}{2} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_t\\|_{U_t}^2 - \\frac{1}{2} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|_{U_t}^2\\\\\n      \\le {} & \\frac{1}{2} \\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_1\\|_{V_1}^2 + \\frac{1}{2} \\sum_{t=1}^T(\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|_{U_{t+1}}^2 - \\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|_{U_t}^2)\\\\\n      \\le {} & \\frac{1}{2} \\gamma D^2 + \\frac{\\alpha G^2 D^2}{4} + \\frac{\\alpha}{4} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|_{\\nabla f_t(\\mathbf{x}_t) \\nabla f_t(\\mathbf{x}_t)^\\top}^2.\n  \n\\end{aligned}')
('11.1', '\\begin{aligned}\n      & \\textsc{Opt-Gap} - \\textsc{Negativity}\\\\\n      \\le {} & \\frac{1}{2} \\gamma D^2 + \\frac{\\alpha}{4} \\sum_{t=1}^T\\|\\mathbf{x}^\\star- \\widehat{\\mathbf{x}}_{t+1}\\|_{\\nabla f_t(\\mathbf{x}_t) \\nabla f_t(\\mathbf{x}_t)^\\top}^2 - \\frac{\\alpha}{2} \\sum_{t=1}^T\\undefined {\\mathbf{x}_t - \\mathbf{x}^\\star}^2_{\\nabla f_t(\\mathbf{x}_t) \\nabla f_t(\\mathbf{x}_t)^\\top} + \\mathcal{O}(1)\\\\\n      \\le {} & \\frac{1}{2} \\gamma D^2 + \\frac{\\alpha}{2} \\sum_{t=1}^T\\|\\mathbf{x}_t - \\widehat{\\mathbf{x}}_{t+1}\\|_{\\nabla f_t(\\mathbf{x}_t) \\nabla f_t(\\mathbf{x}_t)^\\top}^2 + \\mathcal{O}(1),\n  \n\\end{aligned}')

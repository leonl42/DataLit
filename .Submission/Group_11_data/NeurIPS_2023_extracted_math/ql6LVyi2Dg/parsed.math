('1.0.3.1', "\\begin{aligned}\n            &\\quad\\left(\\mathds{E}_{\\mathcal{A}}\\left\\|\\sum_{t=0}^{T-1}\\left(\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\nabla_{\\bm{x}}F_{\\mathcal{S}}(\\bm{x}^t,\\bm{y}^t)\\right)\\right\\|\\right)^2\\\\\n            &\\leq\\mathds{E}_{\\mathcal{A}}\\left\\|\\sum_{t=0}^{T-1}\\left(\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\nabla_{\\bm{x}}F_{\\mathcal{S}}(\\bm{x}^t,\\bm{y}^t)\\right)\\right\\|^2\\\\\n            &=\\sum_{t=0}^{T-1}\\mathds{E}_{\\mathcal{A}}\\left\\|\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\frac{1}{m}\\sum_{i=1}^m\\frac{1}{n}\\sum_{l=1}^n\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,l})\\right\\|^2\\\\\n            &\\quad+\\sum_{t\\neq t'}\\mathds{E}_{\\mathcal{A}}\\langle\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\frac{1}{m}\\sum_{i=1}^m\\frac{1}{n}\\sum_{l=1}^n\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,l}),\\\\\n            &\\quad\\quad\\quad\\quad\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'}_i,\\bm{y}^{t'}_i;\\xi_{i,j_{t'}(i)})-\\frac{1}{m}\\sum_{i=1}^m\\frac{1}{n}\\sum_{l=1}^n\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,l})\\rangle\\\\\n            &\\leq4G^2T+\\sum_{t\\neq t'}\\mathds{E}_{\\mathcal{A}}\\langle\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,j_t(i)})\\\\&\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad+\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,j_t(i)})-\\frac{1}{m}\\sum_{i=1}^m\\frac{1}{n}\\sum_{l=1}^n\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,l}),\\\\\n            &\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'}_i,\\bm{y}^{t'}_i;\\xi_{i,j_{t'}(i)})-\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,j_{t'}(i)})\\\\&\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad+\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,j_{t'}(i)})-\\frac{1}{m}\\sum_{i=1}^m\\frac{1}{n}\\sum_{l=1}^n\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,l})\\rangle\\\\\n            &\\overset{(a)}{=}4G^2T+\\sum_{t\\neq t'}\\mathds{E}_{\\mathcal{A}}\\langle\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,j_t(i)}),\\\\\n            &\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'}_i,\\bm{y}^{t'}_i;\\xi_{i,j_{t'}(i)})-\\frac{1}{m}\\sum_{i=1}^m\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,j_{t'}(i)})\\rangle\\\\\n            &\\leq 4G^2T+\\sum_{t\\neq t'}\\mathds{E}_{\\mathcal{A}}\\left(\\left\\|\\frac{1}{m}\\sum_{i=1}^m\\left(\\nabla_{\\bm{x}}f_i(\\bm{x}^t_i,\\bm{y}^t_i;\\xi_{i,j_t(i)})-\\nabla_{\\bm{x}}f_i(\\bm{x}^t,\\bm{y}^t;\\xi_{i,j_t(i)})\\right)\\right\\|\\cdot\\right.\\\\\n            &\\left.\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\quad\\left\\|\\frac{1}{m}\\sum_{i=1}^m\\left(\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'}_i,\\bm{y}^{t'}_i;\\xi_{i,j_{t'}(i)})-\\nabla_{\\bm{x}}f_i(\\bm{x}^{t'},\\bm{y}^{t'};\\xi_{i,j_{t'}(i)})\\right)\\right\\|\\right)\\\\\n            &\\overset{(b)}{\\leq}4G^2T+\\sum_{t\\neq t'}\\frac{L^2}{m}\\mathds{E}_{\\mathcal{A}}\\left[\\sum_{i=1}^m(\\|\\bm{x}^t-\\bm{x}^t_i\\|^2+\\|\\bm{y}^t-\\bm{y}^t_i\\|^2)\\cdot\\sum_{i=1}^m(\\|\\bm{x}^{t'}-\\bm{x}^{t'}_i\\|^2+\\|\\bm{y}^{t'}-\\bm{y}^{t'}_i\\|^2)\\right]^{1/2}\\\\\n            &\\leq4G^2T+\\sum_{t\\neq t'}4G^2L^2\\left(\\sum_{s=0}^{t-1}\\eta_{s}^{max}\\lambda^{t-1-s}\\right)\\left(\\sum_{s=0}^{t'-1}\\eta_{s}^{max}\\lambda^{t-1-s}\\right)\n        \\end{aligned}")

('11.0', '\\label{equ:gradientboundq}\n\\begin{split}\n &   \n\\left\\lVert\\nabla_{{\\bm{W}}_Q}\\ell({\\bm{\\theta}})\\right\\rVert_\\mathrm{F}  = \\left\\lVert-\\sum^{N}_{n=1}(f({\\bm{X}}_n)-y_n)\\frac{\\partial f({\\bm{X}}_n)}{\\partial  {\\bm{W}}_Q} \\right\\rVert_\\mathrm{F}\n\\\\& = \n    \\tau_0 \\tau_1\n    \\left\\lVert\n    \\sum^{N}_{n=1}(f({\\bm{X}}_n)-y_n)\n    \\sum_{i=1}^{d_s}\n    {\\bm{W}}_k {\\bm{X}}_n^\\top \n        \\left(\n      \\mathrm{diag}({\\bm{\\beta}}_{i,n})-\n        {\\bm{\\beta}}_{i,n} {\\bm{\\beta}}_{i,n}^\\top\n    \\right) {\\bm{X}}_n {\\bm{W}}_V^\\top\n        \\left(\n    {\\bm{w}}_O \\circ \n    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}\n    \\right)\\right){\\bm{X}}_n^{(i,:)}\n    \\right\\rVert_\\mathrm{F}\n\\\\& \\leq 2\n    \\tau_0 \\tau_1\n    \\sum^{N}_{n=1}\n    \\left|(f({\\bm{X}}_n)-y_n)\n    \\right|\n    d_s \\bar{\\lambda}_K\n    \\bar{\\lambda}_V\n    \\bar{\\lambda}_O\n    (C_x \\sqrt{d_s})^3\n\\\\&\n\\leq 2\n    \\tau_0 \\tau_1\n    \\sqrt{\n    N\n    \\sum^{N}_{n=1}\n    \\left|(f({\\bm{X}}_n)-y_n)\n    \\right|^2\n    }\n    d_s \\bar{\\lambda}_K\n    \\bar{\\lambda}_V\n    \\bar{\\lambda}_O\n    (C_x \\sqrt{d_s})^3\n\\\\&= \n    2 \n        \\rho\n    \\tau_0\n     \\bar{\\lambda}_K\n    \\bar{\\lambda}_V\n    \\bar{\\lambda}_O\n    d_s C_x^2\n    \\left\\lVert{\\bm{f}}-{\\bm{y}}\\right\\rVert_2\\,.\n\\end{split}')
('11.0', '{\\bm{W}}_K')
('11.0', '\\label{equ:gradientboundk}\n\\left\\lVert\\nabla_{{\\bm{W}}_K}\\ell({\\bm{\\theta}})\\right\\rVert_\\mathrm{F} \\leq \n    2 \n     \\rho\n    \\tau_0 \n      \\bar{\\lambda}_Q\n    \\bar{\\lambda}_V\n    \\bar{\\lambda}_O\n    d_s C_x^2\n    \\left\\lVert{\\bm{f}}-{\\bm{y}}\\right\\rVert_2\\,.')

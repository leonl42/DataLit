('11.0', "\\small\n\\begin{split}\n&\\left\\lVert{\\bm{h}}_{i,n}^{t'}  -{\\bm{h}}_{i,n}^{t} \\right\\rVert_2\n \\leq\n    \\left\\lVert{\\bm{W}}_V^{t'}\\right\\rVert_2\n    \\left\\lVert        {\\bm{w}}_O^{t'} \\circ \n        \\dot{\\sigma_r}\n        \\left({\\bm{W}}_V^{t'} {\\bm{X}}_n^\\top\n        {\\bm{\\beta}}_{i.n}^{t'}\n        \\right) -  {\\bm{w}}_O^{t} \\circ \n        \\dot{\\sigma_r}\n        \\left(\\tilde{\\bm{W}}_V^{t}{\\bm{X}}_n^\\top\n        {\\bm{\\beta}}_{i.n}^{t}\n        \\right)\\right\\rVert_2\n    + \n    \\left\\lVert{\\bm{W}}_V^{t'}-  {\\bm{W}}_V^{t}\\right\\rVert_2\n    \\left\\lVert{\\bm{w}}_O\\right\\rVert_2\n\\\\& \\leq\n    \\left[\n     \\bar{\\lambda}_V\n     \\left(\n     \\bar{\\lambda}_O \n    C_x \\sqrt{d_s} (1+\n    \\bar{\\lambda}_V\n    z) + 1\n    \\right)\n    +\n   \\bar{\\lambda}_O  \n    \\right]\n    \\left\\lVert{\\bm{\\theta}}^{t'} -{\\bm{\\theta}}^{t}\\right\\rVert_2\\,.\n\\end{split}")
('11.0', "\\begin{split}\n&\\left\\lVert{\\bm{U}}_{i,n}^{t'}-{\\bm{U}}_{i,n}^{t}\\right\\rVert_2  \n\\\\& \\leq\n\\left\\lVert {\\bm{W}}_K^{t'} {\\bm{X}}_n^\\top \n        \\left(\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t'})-\n        {\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top\n    \\right) - \n    {\\bm{W}}_K^{t} {\\bm{X}}_n^\\top \n        \\left(\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t})-\n        {\\bm{\\beta}}_{i,n}^{t} {{\\bm{\\beta}}_{i,n}^{t}}^\\top\n    \\right)\\right\\rVert_2 \n    \\left\\lVert{\\bm{X}}_n\\right\\rVert_2\n\\\\&\n    \\leq C_x \\sqrt{d_s}\n    %\n    \\left[\n    \\left\\lVert{\\bm{W}}_K^{t'} - {\\bm{W}}_K^{t}\\right\\rVert_2\n    \\left\\lVert{\\bm{X}}_n\\right\\rVert_2\n    \\left\\lVert\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t'})-\n        {\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top\n     \\right\\rVert_2\n     \\right.\n\\\\& \n    \\left. \\qquad  \\qquad +\n    \\left\\lVert{\\bm{W}}_K^t\\right\\rVert_2\n    \\left\\lVert{\\bm{X}}_n\\right\\rVert_2\n    \\left\\lVert\n          \\text{diag}({\\bm{\\beta}}_{i,n}^{t'})-\n        {\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top\n        -\n              \\text{diag}({\\bm{\\beta}}_{i,n}^{t})-\n        {\\bm{\\beta}}_{i,n}^{t} {{\\bm{\\beta}}_{i,n}^{t}}^\\top\n    \\right\\rVert_2\n    \\right]\n\\\\ & \\le \n    C_x^2 d_s \\left[\n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^{t}\\right\\rVert_2\n    +\n    \\bar{\\lambda}_K \\left(\n    \\left\\lVert     \\text{diag}({\\bm{\\beta}}_{i,n}^{t'}) -  \\text{diag}({\\bm{\\beta}}_{i,n}^{t})\\right\\rVert_2\n    +\n    \\left\\lVert{\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top - {\\bm{\\beta}}_{i,n}^{t} {{\\bm{\\beta}}_{i,n}^{t}}^\\top\\right\\rVert_2\n    \\right)\n    \\right]\n\\\\ & \\le \n    C_x^2 d_s \\left[\n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^{t}\\right\\rVert_2\n    +\n    \\bar{\\lambda}_K \\left(\n    \\left\\lVert   {\\bm{\\beta}}_{i,n}^{t'}-{\\bm{\\beta}}_{i,n}^{t}  \\right\\rVert_{\\infty}\n    +\n    \\left(\\left\\lVert{\\bm{\\beta}}_{i,n}^{t'}\\right\\rVert_2 +  \\left\\lVert{\\bm{\\beta}}_{i,n}^{t}\\right\\rVert_2\\right)\n    \\left\\lVert{\\bm{\\beta}}_{i,n}^{t'}-{\\bm{\\beta}}_{i,n}^{t}\\right\\rVert_2\n    \\right)\n    \\right]\n\\\\ & \\le \n        C_x^2 d_s \\left[\n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^{t}\\right\\rVert_2\n    +\n    3\\bar{\\lambda}_K \n    \\left\\lVert   {\\bm{\\beta}}_{i,n}^{t'}-{\\bm{\\beta}}_{i,n}^{t}  \\right\\rVert_2\n    \\right]\n\\\\ & \\le \n     C_x^2 d_s \\left[\n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^{t}\\right\\rVert_2\n    +\n    3\\bar{\\lambda}_K \n    2 \\tau_0 C_x^2 d_s \n            \\left(\\bar{\\lambda}_Q\n            \\left\\lVert{\\bm{W}}_K^{t'} - {\\bm{W}}_K^{t}\\right\\rVert_2\n            +\n           \\bar{\\lambda}_K\n            \\left\\lVert{\\bm{W}}_Q^{t'}-{\\bm{W}}_Q^{t}\\right\\rVert_2\n            \\right) \n    \\right]\n\\\\ & \\le \n    [\n    C_x^2 d_s + 3\\bar{\\lambda}_K \n    z\n    ]\n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^{t}\\right\\rVert_2\\,,\n\\end{split}")
('11.0', "\\begin{split}\n&   \\left\\lVert\n    \\nabla_{{\\bm{W}}_Q}f({\\bm{X}}_n;{\\bm{\\theta}}^{t'})\n    -\n    \\nabla_{{\\bm{W}}_Q}f({\\bm{X}}_n;{\\bm{\\theta}}^{t})\n    \\right\\rVert_\\mathrm{F}\n \\\\&\\leq \n        \\tau_0 \\tau_1 C_x d_s\n        \\left\\{\n        2  \\bar{\\lambda}_K d_s C_x^2 \n          \\left[\n     \\bar{\\lambda}_V\n     \\left(\n     \\bar{\\lambda}_O \n    C_x \\sqrt{d_s} (1+\n    \\bar{\\lambda}_V\n    z) + 1\n    \\right)+\n   \\bar{\\lambda}_O  \n    \\right]\n+\n        \\bar{\\lambda}_V\n        \\bar{\\lambda}_O\n    [\n    C_x^2 d_s + 3\\bar{\\lambda}_K  z\n    ]\n        \\right\\}\n    \\left\\lVert{\\bm{\\theta}}^{t'} -{\\bm{\\theta}}^{t}\\right\\rVert_2\\,.\n\\end{split}")

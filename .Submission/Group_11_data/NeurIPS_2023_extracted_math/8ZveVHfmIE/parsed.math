('11.0', "\\label{equ:local_subpart2_new}\n    \\begin{split}\n&\n\\left\\lVert\n    {\\bm{w}}_O^{t'} \\circ \n    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t'}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t'}\n    \\right)\n        -\n    {\\bm{w}}_O^{t} \\circ \n    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t}\n    \\right)\n    \\right\\rVert_2\n\\\\& \\le\n       \\left\\lVert{\\bm{w}}_O^{t'}\\right\\rVert_2\n    \\left\\lVert    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t'}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t'}\n    \\right)\n    -\n        \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V{\\bm{X}}_n^\\top\n   {\\bm{\\beta}}_{i,n}^t\n    \\right)\n    \\right\\rVert_2\n    +\n    \\left\\lVert        \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t}\n    \\right)\n    \\circ\n    \\left({\\bm{w}}_O^{t'} -  {\\bm{w}}_O^{t}\\right)\n    \\right\\rVert_2\n\\\\& \\le\n    \\bar{\\lambda}_O \n    \\left\\lVert{\\bm{W}}_V^{t'}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t'} - \n    {\\bm{W}}_V^{t}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t}\n    \\right\\rVert_2\n    +\n    \\left\\lVert{\\bm{w}}_O^{t'}  - {\\bm{w}}_O^{t} \\right\\rVert_2    \n\\\\& \\le\n    \\bar{\\lambda}_O \n    C_x \\sqrt{d_s}\n        \\left( \n    \\left\\lVert{\\bm{W}}_V^{t'} - {\\bm{W}}_V^{t}\\right\\rVert_2\n    +\n    \\bar{\\lambda}_V\n    2 \\tau_0 C_x^2 d_s \n                \\left(\\bar{\\lambda}_Q\n                \\left\\lVert{\\bm{W}}_K^{t'} - {\\bm{W}}_K^{t}\\right\\rVert_2\n                +\n              \\bar{\\lambda}_K\n                \\left\\lVert{\\bm{W}}_Q^{t'}-{\\bm{W}}_Q^{t}\\right\\rVert_2\n                \\right) \n    \\right)  \n    +\n    \\left\\lVert{\\bm{w}}_O^{t'}  - {\\bm{w}}_O^{t} \\right\\rVert_2\n\\\\& \\le\n    \\left[\n     \\bar{\\lambda}_O \n    C_x \\sqrt{d_s} (1+\n    \\bar{\\lambda}_V\n    z) + 1\n    \\right] \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^\\top \\right\\rVert_2\\,.\n    \\end{split}")
('11.0', "\\begin{split}\n&\\left\\lVert\n    \\nabla_{{\\bm{W}}_V}f({\\bm{X}}_n;{\\bm{\\theta}}^{t'})\n    -\n    \\nabla_{{\\bm{W}}_V}f({\\bm{X}}_n;{\\bm{\\theta}}^{t})\n    \\right\\rVert_\\mathrm{F}\n \\\\& \\le   \n       \\tau_1 C_x\n       d_s^{3/2}\n       \\bigg[\n       \\bar{\\lambda}_O\n    z\n    +\n     \\bar{\\lambda}_O \n    C_x \\sqrt{d_s} (1+\n    \\bar{\\lambda}_V\n    z) + 1\n    \\bigg] \n    \\left\\lVert{\\bm{\\theta}}^{t'}-{\\bm{\\theta}}^\\top \\right\\rVert_2\\,.\n    \\end{split}")
('11.0', "\\begin{split}\n &   {\\bm{U}}_{i,n}^{t'} = {\\bm{W}}_K^{t'} {\\bm{X}}_n^\\top \n        \\left(\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t'})-\n        {\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top\n    \\right) {\\bm{X}}_n ,\\quad \n    {\\bm{h}}_{i,n}^{t'} = \n         {{\\bm{W}}_V^{t'}}^\\top\n        \\left(\n    {\\bm{w}}_O^{t'} \\circ \n    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t'} {\\bm{X}}_n^\\top\n   {\\bm{\\beta}}_{i,n}^{t'}\n    \\right)\\right)\\,,\n\\\\&\n{\\bm{U}}_{i,n}^{t} = {\\bm{W}}_K^{t} {\\bm{X}}_n^\\top \n        \\left(\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t})-\n        {\\bm{\\beta}}_{i,n}^{t} {{\\bm{\\beta}}_{i,n}^{t}}^\\top\n    \\right) {\\bm{X}}_n ,\\quad \n  {\\bm{h}}_{i,n}^{t}= \n         {{\\bm{W}}_V^{t}}^\\top\n        \\left(\n    {\\bm{w}}_O^{t} \\circ \n    \\dot{\\sigma_r}\n    \\left({\\bm{W}}_V^{t}{\\bm{X}}_n^\\top\n    {\\bm{\\beta}}_{i,n}^{t}\n    \\right)\\right)\\,.\n    \\end{split}")
('11.0', "\\label{equ:local_subpart3_new}\n    \\begin{split}\n&       \\left\\lVert\n    \\nabla_{{\\bm{W}}_Q}f({\\bm{X}}_n;{\\bm{\\theta}}^{t'})\n    -\n    \\nabla_{{\\bm{W}}_Q}f({\\bm{X}}_n;{\\bm{\\theta}}^{t})\n    \\right\\rVert_\\mathrm{F}\n=\n    \\tau_0 \\tau_1\n     \\left\\lVert\n    \\sum_{i=1}^{d_s}\n    {\\bm{U}}_{i,n}^{t'}{\\bm{h}}_{i,n}^{t'} {\\bm{X}}_n^{(i,:)} -\\sum_{i=1}^{d_s}{\\bm{U}}_{i,n}^{t}{\\bm{h}}_{i,n}^{t} {\\bm{X}}_n^{(i,:)}\n    \\right\\rVert_\\text{F}\n\\\\& \\leq \n    \\tau_0 \\tau_1 C_x\n     \\left\\lVert\n    \\sum_{i=1}^{d_s}\n    {\\bm{U}}_{i,n}^{t'}{\\bm{h}}_{i,n}^{t'}  -\\sum_{i=1}^{d_s}{\\bm{U}}_{i,n}^{t}{\\bm{h}}_{i,n}^{t}\n    \\right\\rVert_2\n\\leq \n    \\tau_0 \\tau_1 C_x\n        \\sum_{i=1}^{d_s}\n        \\left(\n        \\left\\lVert{\\bm{U}}_{i,n}^{t'}\\right\\rVert_2 \n        \\left\\lVert{\\bm{h}}_{i,n}^{t'}  -{\\bm{h}}_{i,n}^{t} \\right\\rVert_2\n        +\n        \\left\\lVert{\\bm{h}}_{i,n}^{t} \\right\\rVert_2 \n        \\left\\lVert{\\bm{U}}_{i,n}^{t'} -{\\bm{U}}_{i,n}^{t} \\right\\rVert_2\n        \\right)\\,.\n    \\end{split}")
('11.0', "\\begin{split}\n\\left\\lVert{\\bm{U}}_{i,n}^{t'}\\right\\rVert_2  \\leq \\left\\lVert{\\bm{W}}_K^{t'}\\right\\rVert_2  \\left\\lVert{\\bm{X}}_n\\right\\rVert_2^2\n\\left\\lVert\n      \\text{diag}({\\bm{\\beta}}_{i,n}^{t'})-\n     {\\bm{\\beta}}_{i,n}^{t'} {{\\bm{\\beta}}_{i,n}^{t'}}^\\top\\right\\rVert_2\n\\leq \n2  \\bar{\\lambda}_K d_s C_x^2 \\,.\n\\end{split}")

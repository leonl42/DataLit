('11.2', "\\begin{aligned}\n    \\mathsf{dist}_d(g, g') &\\coloneqq \\sup_{x \\in \\mathcal{X}, u \\in \\mathcal{U}} \\mathopen{}\\mathclose\\bgroup\\left\\lVert g(x, u) - g'(x, u)\\aftergroup\\egroup\\right\\rVert,\n    & \\mathsf{dist}_p(\\pi, \\pi') &\\coloneqq \\sup_{x \\in \\mathcal{X}, \\theta \\in \\Theta} \\mathopen{}\\mathclose\\bgroup\\left\\lVert\\pi(x, \\theta) - \\pi'(x, \\theta)\\aftergroup\\egroup\\right\\rVert,\\\\*\n    \\mathsf{dist}_c(f, f') &\\coloneqq \\sup_{x \\in \\mathcal{X}, u \\in \\mathcal{U}} \\mathopen{}\\mathclose\\bgroup\\left\\lvert f(x, u) - f'(x, u)\\aftergroup\\egroup\\right\\rvert.\n\\end{aligned}")

('8', '\\begin{aligned}\n\\\n    & \\max_{S_i(x)=0}(S_i(x)+\\lambda (1-S_i(x)))P_i(x) \\nonumber \\\\ \n    =& \\max_{S_i(x)=0} \\lambda (1-S_i(x))P_i(x) \\nonumber \\\\\n    =& \\max_{i} \\lambda (1-S_i(x))P_i(x),\n    \n\\end{aligned}')
('8', '\\begin{aligned}\n    &\\max_{S_i(x)=1}(S_i(x)+ \\lambda(1-S_i(x)))P_i(x) \\nonumber \\\\\n    =&\\max_{S_i(x)=1}S_i(x)P_i(x) \\nonumber \\\\\n    =&\\max_{i}S_i(x)P_i(x).\n    \n\\end{aligned}')

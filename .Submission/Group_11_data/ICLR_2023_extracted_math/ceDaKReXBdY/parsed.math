('7.6.1', '\\label{eqn:thm3-I_1_1}\n\\begin{aligned}\n\\mathbb{E}_t[I_1^t] & \\overset{(a)}{=}-(1-\\eta_1)\\langle \\nabla R({\\bf{w}}_t), \\mathbb{E}_{t}[D_{t}\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}\\nabla f({\\bf{u}}_{i_t})]\\rangle  \\\\\n& = -(1-\\eta_1) \\langle \\nabla R({\\bf{w}}_t), \\mathbb{E}_{t}[D_{t}\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}\\nabla f(g_{i_t}({\\bf{w}}_t))]\\rangle\\\\\n&+ (1-\\eta_1)\\langle \\nabla R({\\bf{w}}_t), \\mathbb{E}_{t}[D_{t}\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}(\\nabla f({\\bf{u}}_{i_t}) -\\nabla f(g_{i_t}({\\bf{w}}_t))]\\rangle \\\\\n& \\leq -(1-\\eta_1)\\|\\nabla R({\\bf{w}}_t)\\|^2_{D_t}\\\\\n&+(1-\\eta_1)\\|D_{t}^{-1/2} \\nabla R({\\bf{w}}_t)\\|\\mathbb{E}_t[\\|D_{t}^{-1/2}\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}(\\nabla f({\\bf{u}}_{i_t}) -\\nabla f(g_{i_t}({\\bf{w}}_t)))\\|] \\\\\n& \\overset{(b)}{\\leq} -(1-\\eta_1)\\|\\nabla R({\\bf{w}}_t)\\|^2_{D_t} +\\frac{(1-\\eta_1)\\| \\nabla R({\\bf{w}}_t) \\|^2_{D_t} }{2}\\\\\n&+ \\frac{(1-\\eta_1)\\mathbb{E}_t[\\|D_{t}^{-1/2}\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}(\\nabla f({\\bf{u}}_{i_t}) -\\nabla f(g_{i_t}({\\bf{w}}_t)))\\|^2]}{2} \\\\\n &\\leq  -\\frac{(1-\\eta_1)}{2}\\|\\nabla R({\\bf{w}}_t)\\|^2_{D_t} + \\frac{ (1-\\eta_1)}{2}\\mathbb{E}_t[\\|\\nabla g_{i_t}({\\bf{w}}_t; \\xi)^{\\top}(\\nabla f({\\bf{u}}_{i_t}) -\\nabla f(g_{i_t}({\\bf{w}}_t))\\|^2_{D_t}] \\\\\n &\\overset{(c)}{\\leq} -\\frac{(1-\\eta_1)}{2}(\\epsilon + C_g^2C_f^2)^{-1/2}\\|\\nabla R({\\bf{w}}_t) \\|^2 +\\frac{1}{2}\\epsilon^{-1/2}C^2_g L_f^2\\mathbb{E}[\\|g_{i_t}({\\bf{w}}_t) - {\\bf{u}}_{i_t}\\|^2] \\\\\n\\end{aligned}')

('9', '\\begin{aligned}\n        & \\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] \\leq \\pi_{\\textup{max}} \\cdot \\textup{ECE}(g_\\phi), \\\\\n        & \\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] \\leq \\pi_{\\textup{max}} \\cdot \\textup{MCE}(g_\\phi).\n    \\end{aligned}')
('9', '\\pi_{\\textup{max}} = \\textup{max}_{(u,i) \\in \\mathcal{D}} \\abs{(\\hat{p}_{u,i}-p_{u,i})(e^{(1)}_{u,i}-e^{(0)}_{u,i}) / \\hat{p}_{u,i}}')
('9', '\\begin{aligned}\n    \\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] & = \\frac{1}{|\\mathcal{D}|} \\Bigl| \\sum_{u,i \\in \\mathcal{D}} \\Big( \n \\frac{\\hat{p}_{u,i}-p_{u,i}}{\\hat{p}_{u,i}} \\Big) (e_{u,i}-\\hat{e}_{u,i}) \\Bigr| \\\\\n    & \\leq \\frac{1}{|\\mathcal{D}|} \\sum_{u,i \\in \\mathcal{D}} \\Bigl| \\frac{\\hat{p}_{u,i}-p_{u,i}}{\\hat{p}_{u,i}} \\Bigr| |e_{u,i}-\\hat{e}_{u,i}|\\\\\n    & = \\frac{1}{|\\mathcal{D}|} \\sum_{u,i \\in \\mathcal{D}} \\Bigl| \\frac{\\hat{p}_{u,i}-p_{u,i}}{\\hat{p}_{u,i}} \\Bigr| |P(r_{u,i}=1)-\\Tilde{r}_{u,i}||e^{(1)}_{u,i}-e^{(0)}_{u,i}| \\\\\n    \\end{aligned}')
('9', '\\begin{aligned}\n\\because e-\\hat{e} & = P(r=1)e^{(1)}+(1-P(r=1))e^{(0)} - \\Tilde{r}e^{(1)}-(1-\\Tilde{r})e^{(0)} \\\\\n& = (P(r=1)-\\Tilde{r})(e^{(1)}-e^{(0)})\n    \\end{aligned}')
('9', '\\pi_{\\textup{max}} = \\text{max}_{(u,i) \\in \\mathcal{D}} \\abs{(\\hat{p}_{u,i}-p_{u,i})(e^{(1)}_{u,i}-e^{(0)}_{u,i}) / \\hat{p}_{u,i}}')
('9', '\\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] \\leq \\pi_{\\textup{max}} \\cdot \\frac{1}{|\\mathcal{D}|} \\sum_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}|.')
('9', '\\text{ECE}(g_\\phi) = \\frac{1}{|\\mathcal{D}|} \\sum_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}|.')
('9', '\\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] \\leq \\pi_{\\textup{max}} \\cdot \\textup{ECE}(g_\\phi).')
('9', '\\begin{aligned}\n    \\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] & \\leq \\pi_{\\textup{max}} \\cdot \\frac{1}{|\\mathcal{D}|} \\sum_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}| \\\\\n    & \\leq \\pi_{\\textup{max}} \\cdot \\max_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}| \\\\\n\\end{aligned}')
('9', '\\begin{aligned}\n    \\text{MCE}(g_\\phi) &  = \\max_{\\Tilde{r} \\in [0,1]} \\abs{\\mathbb{E}[r|g_\\phi(x)=\\Tilde{r}] - \\Tilde{r}} \\\\\n    & = \\max_{\\Tilde{r} \\in [0,1]} \\abs{P(r=1|g_\\phi(x)=\\Tilde{r}) - \\Tilde{r}} \\\\\n    & = \\max_{u,i \\in \\mathcal{D}} \\abs{P(r_{u,i}=1) - \\Tilde{r}_{u,i}}. \\\\ \n    \\end{aligned}')
('9', '\\textup{Bias}[\\mathcal{E}_{\\textup{DR}}] \\leq \\pi_{\\textup{max}} \\cdot \\textup{MCE}(g_\\phi).')
('9', '\\begin{aligned}\n        & \\textup{Var}[\\mathcal{E}_{\\textup{DR}}] \\leq \\omega_{\\textup{max}} \\cdot \\big( \\textup{ECE}(g_\\phi) \\big)^2, \\\\\n        & \\textup{Var}[\\mathcal{E}_{\\textup{DR}}] \\leq \\frac{\\omega_{\\textup{max}}}{|\\mathcal{D}|} \\cdot \\big( \\textup{MCE}(g_\\phi) \\big)^2,\n    \\end{aligned}')
('9', '\\omega_{\\textup{max}} = \\textup{max}_{(u,i) \\in \\mathcal{D}} \\abs{p_{u,i}(1-p_{u,i})(e^{(1)}_{u,i}-e^{(0)}_{u,i})^2 / \\hat{p}^2_{u,i}}')
('9', '\\begin{aligned}\n    \\textup{Var}[\\mathcal{E}_{\\textup{DR}}] & = \\frac{1}{|\\mathcal{D}|^2} \\sum_{u,i \\in \\mathcal{D}} \\frac{ p_{u,i} (1-p_{u,i})}{\\hat{p}^2_{u,i}} (\\hat{e}_{u,i}-e_{u,i})^2 \\\\\n    & = \\frac{1}{|\\mathcal{D}|^2} \\sum_{u,i \\in \\mathcal{D}} \\Bigl| \\frac{ p_{u,i} (1-p_{u,i})}{\\hat{p}^2_{u,i}} \\Bigr| |P(r_{u,i}=1)-\\Tilde{r}_{u,i}|^2 |e^{(1)}_{u,i}-e^{(0)}_{u,i}|^2 \\\\\n    & \\leq \\omega_{\\textup{max}} \\cdot \\frac{1}{|\\mathcal{D}|^2} \\sum_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}|^2 \\\\\n    & \\leq \\omega_{\\textup{max}} \\cdot \\frac{1}{|\\mathcal{D}|^2} \\biggr( \\sum_{u,i \\in \\mathcal{D}} |P(r_{u,i}=1)-\\Tilde{r}_{u,i}| \\biggr) ^2 \\\\\n    & = \\omega_{\\textup{max}} \\cdot \\big( \\textup{ECE}(g_\\phi) \\big)^2,\n\\end{aligned}')
('9', '\\omega_{\\textup{max}} = \\text{max}_{(u,i) \\in \\mathcal{D}} \\abs{p_{u,i}(1-p_{u,i})(e^{(1)}_{u,i}-e^{(0)}_{u,i})^2 / \\hat{p}^2_{u,i}}')

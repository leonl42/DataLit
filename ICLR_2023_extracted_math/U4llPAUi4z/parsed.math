('9.2', '\\begin{aligned}\n\\label{eq3:lem VX}\n\\left\\|\\bm P \\bm Q^T - \\bm{V}_{L,1}^T\\bm{U}_{L-1,1}\\right\\| = \\left\\|\\bm P\\left( \\bm I - \\bm \\Lambda\\right) \\bm Q^T\\right\\|  = \\| \\bm I - \\bm \\Lambda\\|  \\le \\frac{\\sqrt{2\\delta}\\sqrt[4]{K}}{n^{1/2L}},\n\\end{aligned}')
('9.2', '\\begin{aligned}\n\\label{eq4:lem VX}\n\\bm P \\bm Q^T \\bm{O}_{1} \\tilde{\\bm \\Sigma}^{L-1}_1 \\bm{V}_{1,1}^T\\bar{\\bm x}_k  = \\bm \\Sigma_{L,1}^{-1} \\bm U_L^T\\bm e_k  + \\bm \\xi_k,  \n\\end{aligned}')
('9.2', '\\bm \\xi_k : = ( \\bm P \\bm Q^T - \\bm{V}_{L,1}^T\\bm{U}_{L-1,1} )\\bm{O}_{1} \\tilde{\\bm \\Sigma}^{L-1}_1 \\bm{V}_{1,1}^T\\bar{\\bm x}_k - \\bm V_{L,1}^T\\bm{U}_{L-1,2} \\bm{O}_{2}  \\tilde{\\bm \\Sigma}^{L-1}_2\\bm{V}_{1,2}^T\\bar{\\bm x}_k')
('9.2', '\\begin{aligned}\n\\label{eq5:lem VX}\n\\bm{V}_{1,1}^T\\bar{\\bm x}_k = \\tilde{\\bm \\Sigma}^{-(L-1)}_1\t\\bm{O}_{1}^T\\bm{Q}\\bm{P}^T \\bm \\Sigma_{L,1}^{-1} \\bm U_L^T\\bm e_k  + \\bm \\omega_k, \n\\end{aligned}')

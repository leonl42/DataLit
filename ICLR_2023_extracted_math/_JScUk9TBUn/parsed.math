('11.3', '\\begin{aligned}\n\\sum_{i=1}^N \\mathbb{E}_{\\tilde{\\mu}_t^{(N)}}[ D(\\nu_r,P_{X_i|\\mathscr{X}^{-i}})] \n& = \n\\sum_{i=1}^N \\mathbb{E}_{\\tilde{\\mu}_t^{(N)}}[ \\mathrm{Ent}(P_{X_i|\\mathscr{X}^{-i}})]\n- \\mathbb{E}_{\\tilde{\\mu}_t^{(N)}}\\left[\\sum_{i=1}^N \\log(\\nu_r(X_i))\\right] \\\\\n& \\geq \n\\mathrm{Ent}(\\tilde{\\mu}_t^{(N)})\n- \\mathbb{E}_{\\tilde{\\mu}_t^{(N)}}\\left[\\sum_{i=1}^N \\log(\\nu_r(X_i))\\right]\n= D(\\nu_r^N,\\tilde{\\mu}_t^{(N)}),\n\\end{aligned}')
('11.3', '\\begin{aligned}\n& - \\sum_{i=1}^N \n\\mathbb{E}_{\\mathscr{X}\\sim \\tilde{\\mu}_t^{(N)}}\\left[ \\left\\| \\nabla_i \\log\\left(\\frac{\\tilde{\\mu}_t^{(N)}}{p^{(N)}}(\\mathscr{X}) \\right) \\right\\|^2  \\right]  \\\\\n\\leq  & \n- \\frac{\\alpha }{\\lambda} \n\\left[  N \\mathbb{E}_{\\mathscr{X}\\sim \\tilde{\\mu}_t^{(N)}}[U(\\mu_{\\mathscr{X}})] \n+ \\lambda D(\\nu_r^N,\\tilde{\\mu}_t^{(N)})\n- N(U(\\mu^*) + \\lambda D(\\nu_r,\\mu^*))\n  - 4 L\\left(1 + 2 c_L \\bar{R}^2\\right) %2 L \\bar{R}/\\sqrt{N}\n\\right]\n+ 4 L^2\\bar{R}^2 \\\\\n= & - \\frac{\\alpha }{\\lambda} \n\\left[  N \\mathbb{E}_{\\mathscr{X}\\sim \\tilde{\\mu}_t^{(N)}}[F(\\mu_{\\mathscr{X}})] \n+ \\lambda \\mathrm{Ent}(\\tilde{\\mu}_t^{(N)})\n- N(F(\\mu^*) + \\lambda \\mathrm{Ent}(\\mu^*))\n - 4 L\\left(1 + 2 c_L \\bar{R}^2\\right)  %+ 2 L \\bar{R}/\\sqrt{N}\n\\right]+ 4 L^2\\bar{R}^2 \\\\\n= & - \\frac{\\alpha }{\\lambda} \n\\left( \\mathscr{F}^N(\\tilde{\\mu}_t^{(N)})\n- N \\mathscr{F}(\\mu^*)\n % + 2 L \\bar{R}/\\sqrt{N}\n\\right)\n+ \\frac{4 L \\alpha}{\\lambda}\\left(1 + 2 c_L \\bar{R}^2\\right)\n+ 4 L^2\\bar{R}^2.\n\\end{aligned}')
('11.3', 'C_\\lambda = 2 \\lambda L \\alpha(1 +  2 c_L \\bar{R}^2) + 2 \\lambda^2 L^2\\bar{R}^2.')

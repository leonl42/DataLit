('7.1.6', '\\begin{gathered}\n  \\text{Var}\\!\\left[f\\! \\left( \\mathsf{S}_{1}, \\dots, \\mathsf{S}_{C}  \\right)\\right] = \\text{Var}\\!\\left[f\\! \\left( \\mathbb{E}\\!\\left[\\mathsf{S}_{1}\\right], \\dots, \\mathbb{E}\\!\\left[\\mathsf{S}_{C}\\right] \\right)\\right] + \\sum_{i} \\text{Var}\\!\\left[c_{i} \\frac{\\mathbb{E}\\!\\left[Z_{\\backslash i}\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{2}} \\left( \\mathsf{S}_i- \\mathbb{E}\\!\\left[\\mathsf{S}_i\\right] \\right) \\right] \\\\\n  + \\sum_{i} \\Biggl( \\sum_{j \\neq i} \\left( - c_{j} \\frac{2\\mathbb{E}\\!\\left[\\mathsf{S}_i\\right]c_{i} + \\mathbb{E}\\!\\left[Z\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{3}} \\right)^{2} \\text{Var}\\!\\left[\\left( \\mathsf{S}_i- \\mathbb{E}\\!\\left[\\mathsf{S}_i\\right] \\right) \\left( \\mathsf{S}_j- \\mathbb{E}\\!\\left[\\mathsf{S}_j\\right] \\right)\\right] \\\\\n  + \\left( - 2 c_{i} \\frac{\\mathbb{E}\\!\\left[Z_{\\backslash i}\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{3}} \\right) \\text{Var}\\!\\left[\\left( \\mathsf{S}_i- \\mathbb{E}\\!\\left[\\mathsf{S}_i\\right] \\right)^{2}\\right] \\Biggr) \\quad .\n\\end{gathered}')
('7.1.6', '\\begin{gathered}\n  \\label{eq:second-order-taylor-var-final}\n  \\text{Var}\\!\\left[f\\! \\left( \\mathsf{S}_{1}, \\dots, \\mathsf{S}_{C}  \\right)\\right] = \\sum_{i} \\left( c_{i} \\frac{\\mathbb{E}\\!\\left[Z_{\\backslash i}\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{2}} \\right)^{2} \\text{Var}\\!\\left[\\mathsf{S}_i\\right] \\\\\n  + \\sum_{i} \\Biggl( \\sum_{j \\neq i} \\left( - c_{j} \\frac{2\\mathbb{E}\\!\\left[\\mathsf{S}_i\\right]c_{i} + \\mathbb{E}\\!\\left[Z\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{3}} \\right)^{2} \\left(\\text{Var}\\!\\left[\\mathsf{S}_i\\mathsf{S}_j\\right] - \\mathbb{E}\\!\\left[\\mathsf{S}_i\\right]^{2}\\text{Var}\\!\\left[\\mathsf{S}_j\\right] - \\mathbb{E}\\!\\left[\\mathsf{S}_j\\right]^{2}\\text{Var}\\!\\left[\\mathsf{S}_i\\right]\\right) \\\\\n  + \\left( - 2 c_{i} \\frac{\\mathbb{E}\\!\\left[Z_{\\backslash i}\\right]}{\\mathbb{E}\\!\\left[Z\\right]^{3}} \\right) \\left(\\text{Var}\\!\\left[\\mathsf{S}_i^{2}\\right] - 4\\mathbb{E}\\!\\left[\\mathsf{S}_i\\right]^{2}\\text{Var}\\!\\left[\\mathsf{S}_i\\right]\\right) \\Biggr) \\quad .\n\\end{gathered}')
('7.1.6', '\\begin{aligned}\n  \\label{eq:var-sum-sum}\n  \\text{Var}\\!\\left[\\mathsf{S}_i\\mathsf{S}_j\\right] &= \\text{Var}\\!\\left[\\sum_{k_{i}}w_{k_{i}}^{\\mathsf{S}_i}\\mathsf{N}_{k_{i}}^{\\mathsf{S}_i}  \\sum_{k_{j}}w_{k_{i}}^{\\mathsf{S}_i}\\mathsf{N}_{k_{i}}^{\\mathsf{S}_i}\\right] \\\\\n  &= \\text{Var}\\!\\left[w_{1}^{\\mathsf{S}_i}w_{1}^{\\mathsf{S}_j}\\mathsf{N}_{1}^{\\mathsf{S}_i}\\mathsf{N}_{1}^{\\mathsf{S}_j} + \\dots + w_{1}^{\\mathsf{S}_i}w_{\\left|\\mathsf{S}_j\\right|}^{\\mathsf{S}_j}\\mathsf{N}_{1}^{\\mathsf{S}_i}\\mathsf{N}_{\\left|\\mathsf{S}_j\\right|}^{\\mathsf{S}_j} \\dots + w_{\\left|\\mathsf{S}_i\\right|}^{\\mathsf{S}_i}w_{\\left|\\mathsf{S}_j\\right|}^{\\mathsf{S}_j}\\mathsf{N}_{\\left|\\mathsf{S}_i\\right|}^{\\mathsf{S}_i}\\mathsf{N}_{\\left|\\mathsf{S}_j\\right|}^{\\mathsf{S}_j}\\right] \\\\\n  &= \\sum_{k_{i}}\\sum_{k_{j}} \\text{Var}\\!\\left[w_{k_{i}}^{\\mathsf{S}_i}w_{k_{j}}^{\\mathsf{S}_j}\\mathsf{N}_{k_{i}}^{\\mathsf{S}_i}\\mathsf{N}_{k_{j}}^{\\mathsf{S}_j}\\right] \\\\\n  &= \\sum_{k_{i}}\\sum_{k_{j}} \\left(w_{k_{i}}^{\\mathsf{S}_i}w_{k_{j}}^{\\mathsf{S}_j}\\right)^{2} \\text{Var}\\!\\left[\\mathsf{N}_{k_{i}}^{\\mathsf{S}_i}\\mathsf{N}_{k_{j}}^{\\mathsf{S}_j}\\right] \\quad .\n\\end{aligned}')

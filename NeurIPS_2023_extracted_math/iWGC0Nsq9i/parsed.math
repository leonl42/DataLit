('9.2.0.3', "\\begin{aligned}\n    \\frac{1}{p_t^{\\mathrm{opt}}({\\bm{x}})}\n    & =\n    \\frac{1}{\n    p_t^{\\mathrm{arith-trig}}({\\bm{x}})\n    + \n    O(\\epsilon^{'}({\\bm{x}}))\n    +\n    O(\\epsilon g_1({\\bm{x}}))\n    }\n    =\n    \\frac{1}{\n    p_t^{\\mathrm{arith-trig}}({\\bm{x}})\n    }\n    \\frac{1}{\n    1\n    + \n    \\frac{\n    O(\\epsilon^{'}({\\bm{x}}))\n    +\n    O(\\epsilon g_1({\\bm{x}}))\n    }{\n    p_t^{\\mathrm{arith-trig}}({\\bm{x}})\n    }}\n    \\enspace .\n\\end{aligned}")

('9.2.0.3', "\\begin{aligned}\n    & (\\partial_t \\log p_t({\\bm{x}}))^2 \\times p_t({\\bm{x}})\n    = \n    \\frac{\\big( \\partial_t p_t({\\bm{x}}) \\big)^2}{p_t({\\bm{x}})}\n    \\\\\n    & = \n    \\bigg(\n    \\big(\n    \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}})\n    \\big)^2\n    +\n    O(\\epsilon^{'}({\\bm{x}}))\n    +\n    O(\\epsilon g_2({\\bm{x}}))\n    \\bigg)\n    \\times\n    \\bigg(\n    \\frac{1}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    +\n    \\frac{O(\\epsilon^{''}({\\bm{x}}))}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    \\bigg)\n    \\\\\n    & =\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    +\n    \\frac{\n    O(\\epsilon^{'}({\\bm{x}}))\n    +\n    O(\\epsilon g_2({\\bm{x}}))\n    }{\n    p_t^{\\mathrm{arith-trig}}({\\bm{x}})\n    }\n    +\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    O(\\epsilon^{''}({\\bm{x}}))\n    \\\\\n    & =\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    +\n    O(\\epsilon^{''}({\\bm{x}}))\n    \\bigg(\n    1\n    +\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    \\bigg)\n\\end{aligned}")
('9.2.0.3', "\\begin{aligned}\n    & \\mathrm{MSE}_{\\mathrm{optimal}}\n    \\\\\n    & =\n    \\frac{1}{N} \n    \\int_{\\mathbb{R}^D}\n    \\int_{0}^{1}\n    \\frac{(\\partial_t p_t^{\\mathrm{opt}}({\\bm{x}}))^2}{p_t^{\\mathrm{opt}}({\\bm{x}})}\n    dt d{\\bm{x}}\n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\\\\n    & =\n    \\frac{1}{N} \n    \\int_{\\mathbb{R}^D}\n    \\int_{0}^{1}\n    \\bigg(\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    +\n    O(\\epsilon^{''}({\\bm{x}}))\n    \\bigg(\n    1\n    +\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    \\bigg)\n    \\bigg)\n    dt d{\\bm{x}}\n    \\\\\n    & + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\\\\n    & = \n    \\mathrm{MSE}_{\\mathrm{arith-trig}}\n    +\n    \\frac{1}{N} \n    \\int_{\\mathbb{R}^D}\n    \\int_{0}^{1}\n    O(\\epsilon^{''}({\\bm{x}}))\n    \\bigg(\n    1\n    +\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    \\bigg)\n    dt d{\\bm{x}}\n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\\\\n    & = \n    \\mathrm{MSE}_{\\mathrm{arith-trig}}\n    +\n    O\\bigg( \\frac{\\epsilon^{'''}}{N} \\bigg)\n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n\\end{aligned}")
('9.2.0.3', "\\begin{aligned}\n    \\epsilon^{'''}\n    & = \n    \\int_{\\mathbb{R}^D}\n    \\int_{0}^{1}\n    O(\\epsilon^{''}({\\bm{x}}))\n    \\bigg(\n    1\n    +\n    \\frac{\\big( \\partial_t p_t^{\\mathrm{arith-trig}}({\\bm{x}}) \\big)^2}{p_t^{\\mathrm{arith-trig}}({\\bm{x}})}\n    \\bigg)\n    dt d{\\bm{x}}\n    \\label{eq:definition_epsilon_ppprime}\n\\end{aligned}")
('9.2.0.3', '\\begin{aligned}\n    \\mathrm{MSE}_{\\mathrm{optimal}} \n    & = \n    \\frac{1}{N} 16 \\alpha_H^2 \n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\\\\n    & =\n    \\frac{1}{N} 16 \\bigg( \\frac{\\pi^2}{16} + O(\\epsilon) \n    \\bigg)\n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\\\\n    & =\n    \\frac{1}{N} \\pi^2\n    +\n    O \\bigg( \\frac{\\epsilon}{N} \\bigg)\n    + \n    o \\bigg( \\frac{1}{N} \\bigg)\n    +\n    o \\bigg( \\frac{K^2}{N} \\bigg)\n    \\enspace .\n\\end{aligned}')

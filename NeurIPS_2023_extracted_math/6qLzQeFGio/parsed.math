('1.2', "\\begin{aligned}\n  \\lim_{T\\to\\infty} \\frac{1}{T} {\\mathbf{A}}^{\\rm Unc}_T\n  & = \\lim_{T\\to\\infty} \\frac{1}{T} \\sum_{t=0}^{T-1} {\\boldsymbol{\\phi}}_t \\left(\n  {\\boldsymbol{\\phi}}_t -\n  (1-\\lambda)\\,\\gamma\\sum_{m=1}^{T-t-1} (\\lambda\\,\\gamma)^{m-1} \\, {\\boldsymbol{\\phi}}_{t+m}\n  \\right)^\\top \\label{eq:timeAt}\\\\\n  & = \\sum_{s\\in{\\cal S}} {\\boldsymbol{\\pi}}(s) \\, {\\boldsymbol{\\phi}}(s) \\Bigg(\n  \\sum_{m=0}^\\infty (\\lambda\\,\\gamma)^m \\sum_{s'\\in{\\cal S}} ({\\mathbf{P}}^m)_{s,s'} \\, {\\boldsymbol{\\phi}}(s')\n  - \\gamma\\sum_{m=1}^\\infty (\\lambda\\,\\gamma)^{m-1} \\sum_{s'\\in{\\cal S}} ({\\mathbf{P}}^m)_{s,s'} \\, {\\boldsymbol{\\phi}}(s')\n  \\Bigg)^\\top \\label{eq:ensembleAt}\\\\\n  & = {\\boldsymbol{\\Phi}}^\\top {\\rm Diag}({\\boldsymbol{\\pi}}) \\, ({\\mathbf{I}}-\\lambda\\,\\gamma\\,{\\mathbf{P}})^{-1} \\, {\\boldsymbol{\\Phi}}\n  - \\gamma \\, {\\boldsymbol{\\Phi}}^\\top {\\rm Diag}({\\boldsymbol{\\pi}}) \\, {\\mathbf{P}}\\, ({\\mathbf{I}}-\\lambda\\,\\gamma\\,{\\mathbf{P}})^{-1} \\, {\\boldsymbol{\\Phi}}\\\\\n  & = {\\boldsymbol{\\Phi}}^\\top {\\rm Diag}({\\boldsymbol{\\pi}}) \\, ({\\mathbf{I}}-\\gamma\\,{\\mathbf{P}}) \\, ({\\mathbf{I}}-\\lambda\\,\\gamma\\,{\\mathbf{P}})^{-1} \\, {\\boldsymbol{\\Phi}}\n\\end{aligned}")
('1.2', "\\begin{aligned}\n  \\lim_{T\\to\\infty} \\frac{1}{T} {\\mathbf{b}}_T\n  & = \\lim_{T\\to\\infty} \\frac{1}{T} \\sum_{t=0}^{T-1} {\\boldsymbol{\\phi}}_t \\sum_{m=0}^{T-t-1} (\\lambda\\,\\gamma)^m \\, r_{t+1+m}\n  \\label{eq:timebt}\\\\\n  & = \\sum_{s\\in{\\cal S}} {\\boldsymbol{\\pi}}(s) \\, {\\boldsymbol{\\phi}}(s) \\sum_{m=0}^\\infty (\\lambda\\,\\gamma)^m \\sum_{s'\\in{\\cal S}} (\\mathbf{P}^m)_{s,s'} \\, R(s')\n  \\label{eq:ensemblebt}\\\\\n  & = \\sum_{m=0}^\\infty (\\lambda\\,\\gamma)^m \\, {\\boldsymbol{\\Phi}}^\\top {\\rm Diag}({\\boldsymbol{\\pi}}) \\, {\\mathbf{P}}^m \\, {\\mathbf{r}}\\\\\n  & = {\\boldsymbol{\\Phi}}^\\top {\\rm Diag}({\\boldsymbol{\\pi}}) \\, ({\\mathbf{I}}- \\lambda\\,\\gamma\\,{\\mathbf{P}})^{-1} \\, {\\mathbf{r}},\n\\end{aligned}")
('1.3', '\\begin{aligned}\n  {\\mathbf{A}}^{\\rm Boy}_T\n  & = \\sum_{t=0}^{T-1} {\\boldsymbol{\\phi}}_t \\Bigg(\n  {\\boldsymbol{\\phi}}_t -\n  (1-\\lambda)\\,\\gamma\\sum_{m=1}^{T-t-1} (\\lambda\\,\\gamma)^{m-1} \\, {\\boldsymbol{\\phi}}_{t+m}\n  - \\gamma \\, (\\lambda\\,\\gamma)^{T-t-1} \\, {\\boldsymbol{\\phi}}_{T}\n  \\Bigg)^\\top \\\\\n  & = \\sum_{t=0}^{T-1} {\\boldsymbol{\\phi}}_t \\left(\n  \\sum_{m=0}^{T-t-1} (\\lambda\\,\\gamma)^m \\, {\\boldsymbol{\\phi}}_{t+m}\n  - \\gamma\\sum_{m=1}^{T-t} (\\lambda\\,\\gamma)^{m-1} \\, {\\boldsymbol{\\phi}}_{t+m}\n  \\right)^\\top \\\\\n  & = \\sum_{t=0}^{T-1} {\\boldsymbol{\\phi}}_t \\left(\n  \\sum_{k=t}^{T-1} (\\lambda\\,\\gamma)^{k-t} \\, {\\boldsymbol{\\phi}}_k\n  - \\gamma\\sum_{k=t+1}^{T} (\\lambda\\,\\gamma)^{k-t-1} \\, {\\boldsymbol{\\phi}}_k\n  \\right)^\\top \\\\\n  & =\n  \\sum_{k=0}^{T-1} \\sum_{t=0}^k\n  (\\lambda\\,\\gamma)^{k-t} \\, {\\boldsymbol{\\phi}}_t \\, {\\boldsymbol{\\phi}}_k^\\top\n  - \\gamma \\sum_{k=1}^T \\sum_{t=0}^{k-1}\n  (\\lambda\\,\\gamma)^{k-t-1} \\, {\\boldsymbol{\\phi}}_t \\, {\\boldsymbol{\\phi}}_k^\\top \\label{eq:beforeBoyan}\\\\\n  & = \\sum_{k=0}^{T-1} {\\mathbf{z}}_k \\, {\\boldsymbol{\\phi}}_k^\\top\n  - \\gamma \\sum_{k=1}^T {\\mathbf{z}}_{k-1} \\, {\\boldsymbol{\\phi}}_k^\\top \\label{eq:afterBoyan}\\\\\n  & = \\sum_{k=0}^{T-1} {\\mathbf{z}}_k \\, ({\\boldsymbol{\\phi}}_k - \\gamma \\, {\\boldsymbol{\\phi}}_{k+1})^\\top,\n\\end{aligned}')

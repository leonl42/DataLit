('15', '\\begin{aligned}\n    \\sum_{t=1}^T {\\bf g}_t^\\top({\\widehat{\\bf H}}^{(t)})^\\dag{\\bf g}_t\n    \\leq m-1 + \\frac{m(m-1)}{2}\\ln\\Big(1+\\frac{2\\sum_{t=1}^T\\|{\\bf g}^{(t)}\\|_2^2}{m(m-1)\\sigma^*}\\Big) \\label{bound:pinv02}\n\\end{aligned}')
('15', "t\\leq T'")
('15', '\\alpha^{(0)}=0')

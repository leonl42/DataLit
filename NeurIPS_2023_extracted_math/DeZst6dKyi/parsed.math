('14', '\\begin{aligned}\n         & \\sum_{t=1}^T ({\\bf g}^{(t)})^\\top({\\bf H}^{(t)})^{-1}{\\bf g}^{(t)}\\\\\n        =& \\sum_{t=1}^T \\big\\langle ({\\bf H}^{(t)})^{-1}, {\\bf g}^{(t)}({\\bf g}^{(t)})^\\top \\big\\rangle \\\\\n        =& \\sum_{t=1}^T \\frac{1}{\\mu_t+\\eta_t} \\big\\langle ({\\bf H}^{(t)})^{-1}, {\\bf H}^{(t)}- {\\bf H}^{(t-1)}+   \\frac{1}{2}(\\sigma_m^{(t-1)})^2[{\\bf V}^{(t-1)}_\\perp({\\bf V}^{(t-1)}_\\perp)^\\top-{\\bf V}^{(t-1)}({\\bf V}^{(t-1)})^\\top\\big] \\big\\rangle \\\\\n        \\leq& \\frac{1}{\\mu+\\eta_T}\\sum_{t=1}^T\n                \\big\\langle ({\\bf H}^{(t)})^{-1}, {\\bf H}^{(t)}- {\\bf H}^{(t-1)}\n            + \\frac{1}{2}(\\sigma_m^{(t-1)})^2{\\bf V}^{(t-1)}({\\bf V}^{(t-1)})^\\top \\big\\rangle \\\\\n        =& \\frac{1}{\\mu+\\eta_T}\\sum_{t=1}^T \\Big[\\big\\langle ({\\bf H}^{(t)})^{-1}, {\\bf H}^{(t)}- {\\bf H}^{(t-1)}\\big\\rangle\n            + \\frac{1}{2}(\\sigma_m^{(t-1)})^2{\\rm tr}\\big({\\bf V}^{(t-1)}({\\bf H}^{(t)})^{-1}({\\bf V}^{(t-1)})^\\top\\big) \\Big].\n    \n\\end{aligned}')
('14', '\\eta_t')
('14', '\\sum_{t=1}^T \\langle ({\\bf H}^{(t)})^{-1}, {\\bf H}^{(t)}- {\\bf H}^{(t-1)}\\rangle')
('14', '\\begin{aligned}\n             \\sum_{t=1}^T \\langle ({\\bf H}^{(t)})^{-1}, {\\bf H}^{(t)}- {\\bf H}^{(t-1)}\\rangle\n        & \\leq  \\sum_{t=1}^T \\ln \\frac{\\det({\\bf H}^{(t)})}{\\det({\\bf H}^{(t-1)})} \\\\\n        & = \\ln \\frac{\\det({\\bf H}^{(T)})}{\\det({\\bf H}^{(0)})} \\\\\n        & = \\ln \\frac{\\prod_{i=1}^d\\sigma_i({\\bf H}^{(T)})}{{\\alpha_0}} \\\\\n        & = \\sum_{i=1}^d\\ln \\frac{\\sigma_i\\big(({\\bf B}^{(T)})^\\top{\\bf B}^{(T)}+\\alpha^{(T)}{\\bf I}_d\\big)}{{\\alpha_0}} \\\\\n        & = \\sum_{i=1}^m\\ln \\frac{\\sigma_i\\big(({\\bf B}^{(T)})^\\top{\\bf B}^{(T)}\\big)+\\alpha^{(T)}}{{\\alpha_0}} +\n                (d-m)\\ln\\frac{\\alpha^{(T)}}{{\\alpha_0}} \\\\\n        & \\leq m \\ln  \\frac{\\sum_{i=1}^m [\\sigma_i\\big(({\\bf B}^{(T)})^\\top{\\bf B}^{(T)}\\big)+\\alpha^{(T)}]}{m\\alpha_0} +\n                (d-m)\\ln\\frac{\\alpha^{(T)}}{{\\alpha_0}} \\\\\n         & = m \\ln \\Big(\\frac{{\\rm tr}\\big(({\\bf B}^{(T)})^\\top{\\bf B}^{(T)}\\big)}{m\\alpha_0}+\\frac{\\alpha^{(T)}}{\\alpha_0}\\Big) + (d-m)\\ln\\frac{\\alpha^{(T)}}{{\\alpha_0}}. \\\\\n    \n\\end{aligned}')
('14', '\\frac{1}{2}\\sum_{t=1}^T(\\sigma_m^{(t)})^2{\\rm tr}\\big({\\bf V}^{(t)}({\\bf H}^{(t)})^{-1}({\\bf V}^{(t)})^\\top\\big)')
('14', '\\begin{aligned}\n            \\frac{1}{2}\\sum_{t=1}^T(\\sigma_m^{(t)})^2{\\rm tr}\\big({\\bf V}^{(t)}({\\bf H}^{(t)})^{-1}({\\bf V}^{(t)})^\\top\\big) \\nonumber\n        & \\leq \\frac{1}{2}\\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha^{(t)}} {\\rm tr}\\big({\\bf V}^{(t)}({\\bf V}^{(t)})^\\top\\big) \\nonumber \\\\\n        & = \\frac{m}{2}\\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha^{(t)}}.\n    \n\\end{aligned}')
('14', '\\begin{aligned}\n            R_G\n        \\leq  \\frac{1}{\\mu+\\eta_T}\\Bigg[m \\ln \\Big({\\rm tr}\\big(({\\bf B}^{(T)})^\\top{\\bf B}^{(T)}\\big)+\\frac{\\alpha^{(T)}}{\\alpha_0}\\Big)\n             + (d-m)\\ln\\frac{\\alpha^{(T)}}{{\\alpha_0}} + \\frac{m}{2}\\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha^{(t)}}\\Bigg]. \\label{ieq:RG}\n    \n\\end{aligned}')
('14', 'R_D')
('14', '\\begin{aligned}\n         R_D =& \\sum_{t=1}^T  ({\\bf w}^{(t)}-{\\bf w})^\\top\n            \\big[\\eta_t {\\bf g}^{(t)}({\\bf g}^{(t)})^\\top + \\frac{1}{2}(\\sigma_m^{(t-1)})^2{\\bf I}\n             - {\\bf V}^{(t-1)}({\\bf V}^{(t-1)})^\\top\\big]({\\bf w}^{(t)}-{\\bf w}) \\nonumber\\\\\n          \\leq& \\sum_{t=1}^T \\eta_t({\\bf w}^{(t)}-{\\bf w})^\\top {\\bf g}^{(t)}({\\bf g}^{(t)})^\\top({\\bf w}^{(t)}-{\\bf w})\n             + \\frac{1}{2}\\sum_{t=1}^T(\\sigma_m^{(t-1)})^2({\\bf w}^{(t)}-{\\bf w})^\\top({\\bf w}^{(t)}-{\\bf w}) \\nonumber\\\\\n          \\leq& 4(CL)^2 \\sum_{t=1}^T \\eta_t + 2C^2 \\sum_{t=1}^T(\\sigma_m^{(t-1)})^2. \\label{ieq:RD}\n    \n\\end{aligned}')
('14', '\\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha^{(t)}}')
('14', '\\Omega_{\\rm RFD}')
('14', '{\\mathcal O}(\\ln T)')
('14', '\\sigma_m^{(t)}')
('14', '0 < C_1 \\leq \\big(\\sigma_m^{(t)}\\big)^2 \\leq C_2')
('14', 't=0,\\dots,T-1')
('14', '\\begin{aligned}\n   \\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha^{(t)}}\n& =  \\sum_{t=1}^T\\frac{(\\sigma_m^{(t)})^2}{\\alpha_0+\\frac{1}{2}\\sum_{i=0}^{t-1}\\big(\\sigma_m^{(i)}\\big)^2} \\\\\n& \\leq  \\sum_{t=1}^T\\frac{C_2}{\\alpha_0+\\frac{1}{2}C_1t} \\\\\n& \\leq  \\frac{2C_2}{C_1}\\sum_{t=1}^T\\frac{1}{t} \\\\\n& = {\\mathcal O}(\\ln T).\n\\end{aligned}')
('15', '{\\bf H}^{(t)}')
('15', '\\begin{aligned}\n     {\\bf u}^{(t+1)}&=  {\\bf w}^{(t)}-\\beta_t({\\bf H}^{(t)})^\\dag {\\bf g}^{(t)}, \\nonumber \\\\\n     {\\bf w}^{(t+1)}&= \\mathop {\\rm argmin}_{{\\bf w}\\in{\\mathcal K}_{t+1}} \\| {\\bf w}- {\\bf u}^{(t+1)}\\|_{{\\bf H}^{(t)}}, \\label{update:pinv}\n\\end{aligned}')

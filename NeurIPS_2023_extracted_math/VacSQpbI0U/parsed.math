('11.3', '\\begin{aligned}\n    % &\\bar{\\beta}\\E[\\Big(T(\\mG^T \\mG)\\Big)^{\\dag} \\mG_i^T \\mG_i] = \\mI_d\\\\\n    {\\mathbf{x}}_i &= \\bar{\\beta} \\mathbb{E}[{\\bm{G}}_i^T {\\bm{G}}_i] {\\mathbf{x}}_i \\nonumber\\\\\n    &= \\frac{\\bar{\\beta}}{d} \\mathbb{E}[{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i] {\\mathbf{x}}_i \\nonumber\\\\\n    &= \\frac{\\bar{\\beta}}{d} \\mathbb{E}_{{\\bm{D}}_i} \\Big[ {\\bm{D}}_i {\\bm{H}}^T \\underbrace{\\mathbb{E}[{\\bm{E}}_i^T {\\bm{E}}_i]}_{=(k/d) {\\bm{I}}_d} {\\bm{H}}{\\bm{D}}_i \\Big] {\\mathbf{x}}_i \\tag{$\\because {\\bm{E}}_i$ is independent of ${\\bm{D}}_i$}\\\\\n    % &= \\frac{\\bar{\\beta}}{d} \\sum_{\\mD_i = \\Phi} \\mD_i \\mH^T \\E[\\mE_i^T \\mE_i \\mid \\mD_i = \\Phi] \\mH \\mD_i \\Pr[\\mD_i = \\Phi] \\rvx_i \\nn \\\\\n    % &= \\frac{\\bar{\\beta}}{d} \\sum_{\\mD_i = \\Phi} \\mD_i \\mH^T \\E[\\mE_i^T \\mE_i] \\mH \\mD_i \\Pr[\\mD_i = \\Phi] \\tag{$\\because \\mE_i$ is independent of $\\mD_i$} \\rvx_i \\nn \\\\\n    % &= \\frac{\\bar{\\beta}}{d} \\sum_{\\mD_i = \\Phi} \\mD_i \\mH^T \\frac{k}{d}\\mI_d \\mH \\mD_i \\Pr[\\mD_i = \\Phi] \\rvx_i \\nn \\\\\n    &= \\frac{\\bar{\\beta}}{d} k \\mathbb{E}_{{\\bm{D}}_i} \\left[{\\bm{D}}_i^2 \\right]{\\mathbf{x}}_i \\tag{$\\because$ ${\\bm{H}}^T {\\bm{H}}= d {\\bm{I}}_d$} \\\\\n    % &= \\frac{\\bar{\\beta}}{d} \\frac{k}{d} \\sum_{\\mD_i = \\Phi}\\Pr[\\mD_i = \\Phi] \\mI_d  \\rvx_i \\nn \\\\\n    &= \\frac{\\bar{\\beta} k}{d} {\\mathbf{x}}_i \\tag{$\\because {\\bm{D}}_i^2 = {\\bm{I}}$ is now deterministic.} \\\\\n    \\Rightarrow \\bar{\\beta} &= \\frac{d}{k}. \\label{eq:proof:thm:genspa_nocorr_2}\n\\end{aligned}')
('11.3', '\\begin{aligned}\n    & MSE = \\mathbb{E}\\Big\\|\\widehat{{\\mathbf{x}}} - \\bar{{\\mathbf{x}}} \\Big\\|_2^2 \\nonumber\\\\\n    &= \\mathbb{E}\\Big\\| \\frac{1}{n}\\bar{\\beta}\\sum_{i=1}^{n}\\frac{1}{d}{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i - \\frac{1}{n}\\sum_{i=1}^{n} {\\mathbf{x}}_i \\Big\\|_2^2 \\nonumber\\\\\n    &= \\frac{1}{n^2} \\left\\{ \\mathbb{E}\\Big\\|\\bar{\\beta} \\sum_{i=1}^{n}\\frac{1}{d}{\\bm{D}}_i{\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i \\Big\\|_2^2 \n    + \\Big\\|\\sum_{i=1}^{n}{\\mathbf{x}}_i\\Big\\|_2^2 \\right. \\nonumber\\\\\n    & \\qquad \\qquad \\qquad \\left. - 2\\Big \\langle \\bar{\\beta}\\mathbb{E}[\\sum_{i=1}^{n}\\frac{1}{d}{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i], \\sum_{i=1}^{n} {\\mathbf{x}}_i \\Big\\rangle \\right\\} \\nonumber\\\\\n    &= \\frac{1}{n^2}\\Bigg\\{ \\bar{\\beta}^2 \\mathbb{E}\\Big\\| \\sum_{i=1}^{n} \\frac{1}{d} {\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i \\Big\\|_2^2 - \\Big\\| \\sum_{i=1}^{n} {\\mathbf{x}}_i \\Big\\|_2^2 \\Bigg\\} \\tag{$\\because \\mathbb{E}[\\widehat{{\\mathbf{x}}}] = \\Bar{{\\mathbf{x}}}$} \\\\\n    % &\\text{By the definition of $\\bar{\\beta}$}\\\\\n    &= \\frac{1}{n^2} \\left\\{ \n    \\sum_{i=1}^{n} \\frac{\\bar{\\beta}^2}{d^2} \\mathbb{E}\\Big\\|{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i{\\mathbf{x}}_i \\Big\\|_2^2 - \\sum_{i=1}^{n} \\Big\\|{\\mathbf{x}}_i\\Big\\|_2^2 \\right. \\label{eq:proof:thm:genspa_nocorr_3} \\\\\n    & \\quad \\left. + 2\\sum_{i=1}^{n} \\sum_{l=i+1}^{n} \\frac{\\bar{\\beta}^2}{d^2} \\Big\\langle\n        \\mathbb{E}[{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i], \\mathbb{E}[{\\bm{D}}_l {\\bm{H}}^T {\\bm{E}}_l^T {\\bm{E}}_l {\\bm{H}}{\\bm{D}}_l {\\mathbf{x}}_l]\n    \\Big\\rangle - 2\\sum_{i=1}^{n}  \\sum_{l=i+1}^{n} \\Big\\langle {\\mathbf{x}}_i, {\\mathbf{x}}_l \\Big\\rangle \\right\\}. \\nonumber\n\\end{aligned}')
('11.3', '\\begin{aligned}\n    &\\mathbb{E}\\Big\\|{\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i\\Big\\|_2^2\n    = \\mathbb{E}[{\\mathbf{x}}_i^T {\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i {\\bm{D}}_i {\\bm{H}}^T {\\bm{E}}_i^T {\\bm{E}}_i {\\bm{H}}{\\bm{D}}_i{\\mathbf{x}}_i] \\nonumber\\\\\n    % &= \\E[\\rvx_i^T \\mD_i \\mH^T \\mE_i^T \\mE_i \\mH \\mH^T \\mE_i^T \\mE_i \\mH \\mD_i \\rvx_i]  \\\\\n    &= d \\mathbb{E}[{\\mathbf{x}}_i^T {\\bm{D}}_i {\\bm{H}}^T ({\\bm{E}}_i^T {\\bm{E}}_i)^2 {\\bm{H}}{\\bm{D}}_i {\\mathbf{x}}_i] \\tag{$\\because {\\bm{D}}_i^2 = {\\bm{I}}_d; {\\bm{H}}^T {\\bm{H}}= {\\bm{H}}{\\bm{H}}^T = d {\\bm{I}}_d$} \\\\\n    &= d {\\mathbf{x}}_i^T \\mathbb{E}_{{\\bm{D}}_i} \\left[{\\bm{D}}_i {\\bm{H}}^T \\mathbb{E}[{\\bm{E}}_i^T {\\bm{E}}_i] {\\bm{H}}{\\bm{D}}_i \\right]{\\mathbf{x}}_i \\tag{${\\bm{E}}_i, {\\bm{D}}_i$ are independent; $({\\bm{E}}_i^T {\\bm{E}}_i)^2 = {\\bm{E}}_i^T {\\bm{E}}_i$} \\\\\n    % &= \\sum_{\\mD_i = \\Phi} \\rvx_i^T \\mD_i \\mH^T \\E[(\\mE_i^T \\mE_i)^2 \\mid \\mD_i = \\Phi] \\mH \\mD_i \\rvx_i \\cdot \\Pr[\\mD_i = \\Phi]\\\\\n    % &= \\sum_{\\mD_i = \\Phi} \\rvx_i^T \\mD_i \\mH^T \\E[(\\mE_i^T \\mE_i)^2] \\mH \\mD_i \\rvx_i \\cdot \\Pr[\\mD_i = \\Phi]\\\\\n    % &= \\sum_{\\mD_i = \\Phi} \\rvx_i^T \\mD_i \\mH^T \\frac{k}{d} \\mI_d \\mH \\mD_i \\rvx_i \\cdot \\Pr[\\mD_i = \\Phi]\\\\\n    % &= \\frac{k}{d} \\sum_{\\mD_i = \\Phi} \\rvx_i^T \\mD_i \\mD_i \\rvx_i \\cdot \\Pr[\\mD_i = \\Phi]\\\\\n    % &= \\frac{k}{d} \\sum_{\\mD_i = \\Phi} \\Pr[\\mD_i = \\Phi]\\cdot \\|\\rvx_i\\|_2^2\\\\\n    &= kd \\|{\\mathbf{x}}_i\\|_2^2, \\label{eq:proof:thm:genspa_nocorr_4}\n\\end{aligned}')
('11.3', '\\mathbb{E}[{\\bm{E}}_i^T {\\bm{E}}_i] = (k/d) {\\bm{I}}_d')

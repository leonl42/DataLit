('13.4', '\\begin{aligned}\n    \\left< \\bm{\\hat\\xi}^{\\ell} \\bm{\\hat\\xi}^{\\ell} \\right> &= -\\frac{\\partial^2}{\\partial \\bm r\\partial \\bm r^\\top } \\left< \\exp\\left( i \\hat{\\bm r} \\cdot \\bm{\\hat\\xi}^{\\ell} \\right) \\right>|_{\\bm r=\\bm0}  \\nonumber\n    \\\\\n    &= [\\bm G^{\\ell+1}]^{-1} - [\\bm G^{\\ell+1}]^{-1} \\left< ( \\bm\\xi^{\\ell} - \\bm B^{\\ell \\top} \\bm\\phi^\\ell ) ( \\bm\\xi^{\\ell} - \\bm B^{\\ell \\top} \\bm\\phi^\\ell )^\\top \\right>[\\bm G^{\\ell+1}]^{-1}\n\\end{aligned}')
('13.4', '\\begin{aligned}\n    -i \\left< \\hat{\\bm\\chi}^{\\ell+1} \\bm g^{\\ell+1,\\top} \\right> &= \\frac{\\partial }{\\partial \\bm u} \\left< \\exp\\left( -i \\hat{\\bm u} \\cdot \\bm{\\hat\\chi}^{\\ell+1} \\right) \\bm g^{\\ell+1 \\top}\\right> = [\\bm\\Phi^\\ell]^{-1} \\left< (\\bm\\chi^{\\ell+1} -\\bm A^\\ell \\bm g^{\\ell+1}) \\bm g^{\\ell+1 \\top} \\right> \\nonumber\n    \\\\\n    -i \\left< \\phi(\\bm h^\\ell) \\bm{\\hat\\xi}^{\\ell\\top} \\right> &= \\frac{\\partial }{\\partial \\bm r^\\top} \\left< \\phi(\\bm h) \\exp\\left( -i \\bm r\\cdot \\bm{\\hat\\xi}^\\ell \\right) \\right> = \\left< \\bm\\phi(\\bm h^\\ell) (\\bm\\xi^\\ell - \\bm B^{\\ell\\top} \\bm\\phi(\\bm h^\\ell)) \\right> [\\bm G^{\\ell+1}]^{-1}\n\\end{aligned}')

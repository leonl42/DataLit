('9.0', '\\begin{aligned}\n    &\\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mathsf{Bias}_{t,1}\\rangle\\\\\n    =& \\left\\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}) - \\frac{d}{\\mu}\\mathbb{E}\\left[\\left. \\left( V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) \\right) {\\bm{v}}_t  \\right| {\\mathcal{F}}_t\\right] \\right\\rangle\\\\\n    =& \\frac{d}{\\mu}\\left\\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mathbb{E}\\left[ \\frac{\\mu}{d}\\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}) - \\left. \\left( V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) \\right) {\\bm{v}}_t  \\right| {\\mathcal{F}}_t\\right] \\right\\rangle \\\\\n    =& - \\frac{d}{\\mu}\\mathbb{E}\\left[\\left. \\left( V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) -\\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mu{\\bm{v}}_t\\rangle \\right) \\left\\langle\\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), {\\bm{v}}_t \\right\\rangle \\right| {\\mathcal{F}}_t\\right]\\\\\n    \\leq & \\frac{d}{\\mu}\\mathbb{E}\\left[\\left. \\left| V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) -\\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mu{\\bm{v}}_t\\rangle \\right| \\left\\|\\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}) \\right\\|_2 \\left\\|{\\bm{v}}_t \\right\\|_2 \\right| {\\mathcal{F}}_t\\right].\n\\end{aligned}')

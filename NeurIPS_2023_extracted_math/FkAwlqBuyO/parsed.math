('9.1', '\\begin{aligned}\n    &\\mathbb{E}\\left[\\left.\\left\\| \\left( V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) - \\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mu {\\bm{v}}_t \\rangle \\right){\\bm{v}}_t\\right\\|_2^2 \\right| {\\mathcal{F}}_t \\right]\\\\\n    = &\\mathbb{E}\\left[\\left.\\left| \\left( V(\\pi_{{\\bm{\\theta}}_t + \\mu {\\bm{v}}_t}) - V(\\pi_{{\\bm{\\theta}}_t}) - \\langle \\nabla_{{\\bm{\\theta}}} V(\\pi_{{\\bm{\\theta}}_t}), \\mu {\\bm{v}}_t \\rangle \\right)\\right|_2^2 \\|{\\bm{v}}_t\\|_2^2 \\right| {\\mathcal{F}}_t \\right]\\\\\n    \\leq & \\mathbb{E}\\left[\\left.\\frac{L^2\\mu^4}{4} \\|{\\bm{v}}_t\\|_2^4 \\right| {\\mathcal{F}}_t \\right]\\\\\n    =& \\frac{L^2\\mu^4}{4},\n\\end{aligned}')
